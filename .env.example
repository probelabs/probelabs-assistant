# Slack Configuration
SLACK_APP_TOKEN=xapp-...
SLACK_BOT_TOKEN=xoxb-...
SLACK_SIGNING_SECRET=...

# AI Provider
GOOGLE_API_KEY=...

# Twitter/X API (https://developer.twitter.com/en/portal/dashboard)
TWITTER_CONSUMER_KEY=...
TWITTER_SECRET_KEY=...
